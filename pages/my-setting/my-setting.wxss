.list {
  background: white;
  box-sizing: border-box;
  width: 100%;
  margin-top: 160rpx;
  padding: 0 32rpx;
}

.logout {
  position: absolute;
  margin-left: 70rpx;
  margin-right: 70rpx;
  bottom: 88rpx;
  height: 90rpx;
  min-width: 610rpx;
  background-color: #FFFFFF;
  border-radius: 4rpx;

}

.logout text {
  display: flex;
  position: relative;
  height: 84rpx;
  color: #333333;
  font-size: 30rpx;
  justify-content: center;
  align-items: center;
}

.list-item {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 56rpx 0;
  border-bottom: 1rpx solid rgba(26, 23, 27, 0.12);
}

.list-version {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 56rpx 0;
}

.list-item-title {
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #1A171B;
  line-height: 48rpx;
}

.list-item-version {
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #8E8E8E;
  line-height: 48rpx;
}

.list-right-arrow {
  width: 32rpx;
  height: 32rpx;
}

.footer-wrap {
  padding-bottom: 32rpx;
}